{
    "name": "n8n-nodes-aitable",
    "version": "4.2.2",
    "description": "n8n node for Aitable",
    "keywords": [
        "n8n-community-node-package"
    ],
    "license": "MIT",
    "homepage": "",
    "author": {
        "name": "hungtidus",
        "email": "hungtidus@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hungtidus/n8n-nodes-aitable.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts'",
        "prepublishOnly": "npm run build",
        "prebuild": "npm run lint"
    },
    "files": [
        "dist",
        "index.js"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/AitableApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Aitable/Aitable.node.js"
        ]
    },
    "devDependencies": {
        "@types/express": "^4.17.6",
        "@types/request-promise-native": "~1.0.15",
        "@typescript-eslint/eslint-plugin": "~5.45",
        "@typescript-eslint/parser": "~5.45",
        "eslint": "^8.57.1",
        "eslint-plugin-n8n-nodes-base": "^1.11.0",
        "gulp": "^4.0.2",
        "n8n-core": "*",
        "n8n-workflow": "*",
        "prettier": "^2.7.1",
        "typescript": "~4.8.4"
    },
    "peerDependencies": {
        "n8n-core": "*",
        "n8n-workflow": "*"
    },
    "dependencies": {
        "@azure/storage-blob": "^12.26.0"
    }
}
